[user]
    name = Q
    email = quintonparker@gmail.com
[github]
    user = quintonparker@gmail.com
[color]
    status = auto
    branch = auto
    interactive = auto
    diff = auto
    ui = true
[alias]
    ai = add --interactive
    br = branch
    co = checkout
    ci = commit --verbose
    ctags = !.git/hooks/ctags
    diffws = "diff --ignore-space-at-eol --ignore-space-change --ignore-all-space --ignore-blank-lines"
    lg = log --stat --patch
    rb = rebase
    st = status
    sync = !git-sync
    vidiff = difftool
    vimerge = mergetool
[init]
    templatedir = ~/.git_template
[core]
    excludesfile = ~/.gitignore_global
    editor = /usr/bin/vim
[diff]
    tool = vimdiff
[difftool]
    prompt = false
[merge]
    tool = vimdiff
[credential]
    helper = osxkeychain
[push]
    default = simple
