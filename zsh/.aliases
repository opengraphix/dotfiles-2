alias ls='ls -G'
alias ll='ls -lt'
alias grep='grep --colour=always'
alias ack='ack --colour'
alias less='less -r'
alias diffsbs='diff --side-by-side --suppress-common-lines'
alias hist='history -f -D'

alias x="chmod -v +x"
alias v="vim"
alias c="clear"
alias h="cd"
alias t="tee"

alias gadd='git add'
alias gco='git co'
alias gci='git ci'
alias grb='git rb'
alias gbr='git br'
alias gdi='git diff'
alias gpu='git push'
alias gpl='git pull'
alias glg='git lg'
alias gst='git status'

eval "$(hub alias -s)"
